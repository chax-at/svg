{
  "name": "@dxfom/svg",
  "version": "0.0.0",
  "description": "",
  "private": true,
  "license": "GPL-3.0",
  "repository": "dxfom/svg",
  "keywords": [
    "dxf"
  ],
  "main": "index.mjs",
  "module": "index.mjs",
  "types": "index.d.ts",
  "scripts": {
    "build": "npm-run-all -p typecheck build:*",
    "build:mjs": "rollup -c",
    "build:dts": "dts-bundle-generator --no-banner --inline-declare-externals -o index.d.ts src/index.ts",
    "typecheck": "tsc -p .",
    "start": "npm-run-all -p watch:*",
    "watch:mjs": "rollup -c -w",
    "watch:viewer": "esbuild --bundle --minify --serve --servedir=viewer viewer/app.ts",
    "test": "node --harmony --es-module-specifier-resolution=node bin/dxf2svg.mjs sample/1.dxf"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.13",
    "@babel/plugin-proposal-optional-chaining": "^7.12.16",
    "@babel/plugin-transform-react-jsx": "^7.12.16",
    "@babel/preset-typescript": "^7.12.16",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "dts-bundle-generator": "^5.7.0",
    "esbuild": "^0.8.46",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.39.0",
    "rollup-plugin-babel": "^4.4.0",
    "typescript": "^4.1.5"
  },
  "dependencies": {
    "@dxfom/color": "^0.1.1",
    "@dxfom/dxf": "^0.0.1",
    "@dxfom/mtext": "^0.3.2",
    "@dxfom/text": "^0.2.0"
  }
}
